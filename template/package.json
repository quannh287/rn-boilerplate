{
  "name": "rnbase",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "react-native run-android --variant=developmentDebug --appIdSuffix=dev",
    "android:stage": "react-native run-android --variant=stagingDebug --appIdSuffix=stage",
    "android:prod": "react-native run-android --variant=productionDebug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "format": "prettier --write **/*.{ts,tsx,js,json,md}",
    "test": "jest",
    "lint": "eslint  index.js ./scripts/*.js  ./src/** --ext .js,.ts,.tsx",
    "type-check": "yarn tsc --noEmit --skipLibCheck",
    "postinstall": "cd ios && pod install",
    "splash": "react-native generate-bootsplash splash/splash.png --background-color=FFFFFF --logo-width=150  --assets-path=assets --flavor=main",
    "app-icon": "npx rn-ml appicon -s appicon/appicon.png"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^1.2.1",
    "i18next": "^21.8.14",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "18.0.0",
    "react-fast-compare": "^3.2.0",
    "react-hook-form": "^7.41.0",
    "react-i18next": "^11.18.3",
    "react-native": "0.69.3",
    "react-native-bootsplash": "^4.4.0",
    "react-native-code-push": "^7.0.5",
    "react-native-common-components": "git+https://github.com/quannh287/react-native-common-components.git",
    "react-native-config": "^1.4.11",
    "react-native-fast-image": "^8.6.3",
    "react-native-flipper": "^0.176.1",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-mmkv": "^2.5.1",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-redux": "^8.0.2",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-navigation/devtools": "^6.0.15",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.8",
    "@types/react-native": "^0.70.8",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "patch-package": "^6.5.0",
    "prettier": "^2.7.1",
    "react-native-mmkv-flipper-plugin": "^1.0.0",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.9.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  }
}
